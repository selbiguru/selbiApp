//DEFAULT
"#paymentView":{
	layout: 'vertical',
	backgroundColor: '#FDFDFD'
},
"#viewAddVenmo": {
	backgroundColor: '#3D95CE',
	width: '90%',
	borderRadius: "2",
	borderWidth: "2dp",
	borderColor: "#E1E1EA"
},
"#separatorLabel": {
	font:{
		fontSize: "16dp",
    	fontFamily: 'Nunito-Bold'
	},
	color: "#545555"
},
"#paymentDetails": {
	top: "25dp"
},
".payment-label":{
	textAlign: Titanium.UI.TEXT_ALIGNMENT_LEFT,
	top: "7dp",
	left: "5dp",
	font:{
		fontSize: "16dp",
    	fontFamily: 'Nunito-Bold'
	},
	color: "#545555",
	width: Ti.UI.FILL
	
},
".tableRow":{
	height: '40dp'
},
".rowContainer":{
	height: '40dp',
	layout:'horizontal',
	borderWidth: "1dp",
	borderColor: "#EAEAEA",
},
".rowLabel":{
	left: "15dp",
	height: "40dp",
	font:{
		fontSize: "14dp",
    	fontFamily: 'Nunito-Light'
	},
	color: "#545555"
},
".row-icon":{
	textAlign: Ti.UI.TEXT_ALIGNMENT_RIGHT,
	right: "15dp",
	width: Titanium.UI.FILL,
	color: "#1BA7CD"
},
".menu-underline": {
	height: "1dp",
	top:"0dp",
	left:"15dp",
	width: Titanium.UI.FILL,
	backgroundColor:"#EAEAEA"
},
".topPadding": {
	top: "5dp"
},


//iPhone 4
"#paymentTopBar[if=Alloy.Globals.iPhoneFour]":{
	width: Ti.UI.FILL,
	height: "60dp",
	backgroundColor: '#FAFAFA',
	layout: 'horizontal'
},
"#menuButton[if=Alloy.Globals.iPhoneFour]":{
	height: "60dp",
	width: "60dp",
	backgroundColor: '#FAFAFA'
},
"#paymentButtonIcon[if=Alloy.Globals.iPhoneFour]": {
	font: {
            fontSize: "18dp"
        },
	color: "#1BA7CD"
},
"#titlePaymentView[if=Alloy.Globals.iPhoneFour]": {
