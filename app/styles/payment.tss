},

"#titlePaymentLabel[if=Alloy.Globals.iPhoneFour]": {
	color: "#1BA7CD",
	width: Titanium.UI.SIZE,
	font: {
		fontSize: "18dp",
		fontFamily: "Nunito-Bold"
	}
},
"#imageAddVenmo[if=Alloy.Globals.iPhoneFour]": {
	height: '16dp',
	width: '84dp',
	left: '102dp',
	top: '12dp'
},
".payment-types[if=Alloy.Globals.iPhoneFour]":{
