//DEFAULT
"#aboutUsView": {
	layout: 'vertical',
	backgroundColor: '#FAFAFA'
},
"#activityIndicator": {
	style: 13,
	indicatorDiameter: 2,
	indicatorColor: '#9B9B9B',
	height: Ti.UI.FILL
},











//iPhone 4
"#aboutUsTopBar[if=Alloy.Globals.iPhoneFour]":{
	width: Ti.UI.FILL,
	height: "60dp",
	backgroundColor: '#FFF',
	layout: 'horizontal'
},
"#menuButton[if=Alloy.Globals.iPhoneFour]":{
	height: "60dp",
	width: "60dp",
	backgroundColor: '#FFF'
},
"#aboutUsButtonIcon[if=Alloy.Globals.iPhoneFour]": {
	font: {
            fontSize: "18dp"
        },
	color: "#1BA7CD"
},
"#titleAboutUsView[if=Alloy.Globals.iPhoneFour]": {
	right: "60dp"
},

"#titleAboutUsLabel[if=Alloy.Globals.iPhoneFour]": {
	color: "#1BA7CD",
	width: Titanium.UI.SIZE,
	font: {
		fontSize: "18dp",
		fontFamily: "Nunito-Bold"
	}
},
"#aboutUsInfoView[if=Alloy.Globals.iPhoneFour]": {
	layout: 'vertical',
	width: '85%',
	height: Ti.UI.FILL,
	top:"15dp"
},










