//DEFAULT
"#contactUsView": {
	layout: 'vertical',
	backgroundColor: '#FAFAFA'
},
"#messageView": {
	height: Ti.UI.SIZE,
},











//iPhone 4
"#contactUsTopBar[if=Alloy.Globals.iPhoneFour]":{
	width: Ti.UI.FILL,
	height: "60dp",
	backgroundColor: '#FFF',
	layout: 'horizontal'
},
"#menuButton[if=Alloy.Globals.iPhoneFour]":{
	height: "60dp",
	width: "60dp",
	backgroundColor: '#FFF'
},
"#titleContactUsView[if=Alloy.Globals.iPhoneFour]": {
	width: "200dp",
},
"#titleContactUsLabel[if=Alloy.Globals.iPhoneFour]": {
	color: "#1BA7CD",
	width: Titanium.UI.SIZE,
	font: {
		fontSize: "18dp",
		fontFamily: "Nunito-Bold"
	}
},
"#sendEmailView[if=Alloy.Globals.iPhoneFour]": {
	width: '60dp'
},
".icon-buttons[if=Alloy.Globals.iPhoneFour]": {
	font: {
            fontSize: "18dp"
        },
	color: "#1BA7CD"
},
"#contactUsInfoView[if=Alloy.Globals.iPhoneFour]": {
	layout: 'vertical',
	width: '85%',
	top: '20dp'
},
"#emailTitle[if=Alloy.Globals.iPhoneFour]": {
	borderWidth: '2dp', 
	borderColor: '#E5E5E5',
	backgroundColor: "#FFF",
	width: Titanium.UI.FILL,
	height: '30dp',
	hintText: "Email Subject"
},
"#emailBody[if=Alloy.Globals.iPhoneFour]": {
	borderWidth: '2dp', 
	borderColor: '#E5E5E5',
	width: Titanium.UI.FILL,
	top: '12dp',
	height: '245dp'
},
"#hintTextLabel[if=Alloy.Globals.iPhoneFour]": {
	left: '10dp',
	top:'20dp',
	color: "#CBCBCB"
},
".font-style[if=Alloy.Globals.iPhoneFour]": {
	font: {
		fontSize: '14dp',
		fontFamily: 'Nunito-Light'
	}
},













