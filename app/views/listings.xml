<Alloy>
	<View id="listingsView">
        <View id="listingsTopBar">
			<View id="menuButton">
				<Label icon="fa-arrow-left" id="listingsButtonIcon"></Label>
			<Widget id="fa" src="com.mattmcfarland.fontawesome"/>
			</View>
			<View id="titleListingsView">
				<Label id="titleListingsLabel">Listings</Label>
			</View>
		</View>
        <TableView>
            <TableViewRow id="row">
                <View id="view" onClick="openListing" backgroundColor="red" width="20" height="20"/>
            </TableViewRow>
            <TableViewRow id="row1">
                <View id="view1" backgroundColor="green" width="20" height="20"/>
            </TableViewRow>
        </TableView>
    </View>
</Alloy>