<Alloy>
	<View id="addCreditCardView">
		<View id="addCreditCardTopBar">
			<View id="backViewButton" onClick="backButton">
				<Label icon="fa-reply" id="addCreditCardButtonIcon" class="icon-buttons"></Label>		
			</View>
			<View id="titleAddCreditCardView">
				<Label id="titleAddCreditCardLabel">Add Credit Card</Label>
			</View>
			<View id="saveCreditCardView">
				<Button onClick="saveCreditCard" id="saveCreditCardButton" class="icon-buttons">Save</Button>
			</View>
			<Widget id="fa" src="com.mattmcfarland.fontawesome"/>
		</View>
		<ActivityIndicator id="activityIndicator" message="Loading..."/>
		<View id="enterCCDetailView">
			<WebView id="webview" url="/webViews/addCreditCard.html">
		</WebView>
		</View>
	</View>
</Alloy>