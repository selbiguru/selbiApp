<Alloy>
    <View id="settingsView">
        <View id="settingsTopBar">
			<View id="menuButton">
				<Label icon="fa-arrow-left" id="settingsButtonIcon"></Label>
			<Widget id="fa" src="com.mattmcfarland.fontawesome"/>
			</View>
			<View id="titleSettingsView">
				<Label id="titleSettingsLabel">Settings</Label>
			</View>
		</View>
        <TableView>
            <TableViewRow id="row">
                <View id="view" backgroundColor="red" width="20" height="20"/>
            </TableViewRow>
        </TableView>
    </View>
</Alloy>
