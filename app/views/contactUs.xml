				<Label icon="fa-arrow-left" id="contactUsButtonIcon"></Label>
			<Widget id="fa" src="com.mattmcfarland.fontawesome"/>
			</View>
			<View id="titleContactUsView">
				<Label id="titleContactUsLabel">Contact Us</Label>
			</View>
			<View id="sendEmailView" class="validateButtons">
				<Button onClick="sendEmailToSelbi" id="sendEmailButton" class="send-email-button-icon">Send</Button>
			</View>
		</View>
		<View id="contactUsInfoView">
			
		</View>
    </View>
</Alloy>