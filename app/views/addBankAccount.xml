<Alloy>
	<View id="addBankAccountView">
		<View id="addBankAccountTopBar">
			<View id="menuButton">
				<Label icon="fa-arrow-left" id="addBankAccountButtonIcon"></Label>
			<Widget id="fa" src="com.mattmcfarland.fontawesome"/>			
			</View>
			<View id="titleAddBankAccountView">
				<Label id="titleAddBankAccountLabel">Add Bank Account</Label>
			</View>
		</View>
		<View id="enterBankAccountDetailView">
			<TextField id="accountNumber" placeholder="Account Number"></TextField>
			<TextField id="routingNumber" placeholder="Routing Number"></TextField>
		</View>
	</View>
</Alloy>