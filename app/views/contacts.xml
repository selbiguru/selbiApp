<Alloy>
	<View id="addFriendsView">
		<View id="addFriendsTopBar">
			<View id="menuButton" class="validateButtons">
				<Label icon="fa-arrow-left" id="addFriendsButtonIcon" class="icon-buttons"></Label>
			</View>
			<View id="titleAddFriendsView">
				<Label id="titleAddFriendsLabel">Contact Us</Label>
			</View>
			<Widget id="fa" src="com.mattmcfarland.fontawesome"/>
		</View>
		<View class="container contact-view" layout="vertical" id="contactView" height="70">
			<Label>Your contacts</Label>
			<Button onClick="importContacts">Import your contacts </Button>
		</View>
	</View>
	
</Alloy>